{
    "extends": "@parcel/config-default",
    "compressors": {
        "*.{html,css,js,svg,map,png,jpg,gltf}": [
            "...",
            "@parcel/compressor-gzip",
            "@parcel/compressor-brotli"
        ]
    },
    "transformers": {
        "*.gltf": [
            "@parcel/transformer-raw"
        ]
    }
}
